{
  "manifest_version": 2,

	"name": "Gerrit be nice to me",
	"description": "This makes gerrit ui a little nicer",
	"version": "1.0.1",
	"permissions": [
		"webRequest", "*://gerrit.wikimedia.org/*"
	],
	"content_scripts": [{
		"matches": [ "https://gerrit.wikimedia.org/r/*" ],
		"js": [ "jquery.js", "main.js" ]
	}]
}
